{
  "id": "io.github.deewong7.ai-assisted-image-editing-for-ps-vanilla",
  "name": "AI-assisted Image Editing for Photoshop Vanilla",
  "version": "0.1.4-alpha",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "25.1.0"
  },
  "manifestVersion": 5,
  "requiredPermissions": {
    "network": {
      "domains": [
        "https://dummyjson.com",
        "https://aiplatform.googleapis.com",
        "https://ark.cn-beijing.volces.com"
      ]
    },
    "launchProcess": {
      "schemes": [
        "https"
      ]
    },
    "clipboard": "read"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "vanilla",
      "minimumSize": {
        "width": 253,
        "height": 870
      },
      "maximumSize": {
        "width": 500,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 253,
        "height": 870
      },
      "preferredFloatingSize": {
        "width": 253,
        "height": 300
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_D.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_N.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ],
      "label": {
        "default": "Image Generation"
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium"
      ]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ]
    }
  ]
}
